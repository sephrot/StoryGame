@model IEnumerable<Story>

<section class="pick-wrap">
  <h1 class="page-title">Pick a story</h1>

  <ul class="story-chips">
    @foreach (var story in Model)
    {
      <li><a href="/Story/Story/@story.StoryId" class="chip">@story.Text</a></li>
    }
  </ul>

  <nav class="toolbar">
    <a class="btn btn-success" href="/">Back to Home</a>
  </nav>
</section>
