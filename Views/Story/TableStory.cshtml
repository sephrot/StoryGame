@model IEnumerable<Story>

<section class="table-wrap">
  <header class="masthead">
    <h1 class="page-title">All the stories!</h1>
    <div class="pixel-divider" aria-hidden="true"></div>
  </header>

  <nav class="toolbar" aria-label="Actions">
    <a class="btn btn-success" href="/">Back to Home</a>
    <a class="btn btn-outline-danger" href="/Story/Create">Create a Story</a>
  </nav>

  <ul class="story-chips">
    @foreach (var story in Model)
    {
      <li>
        <a href="/Story/Details/@story.StoryId" class="chip">@story.Text</a>
      </li>
    }
  </ul>
</section>
